import { SinglyLinkedType, Node } from "../interface"

const SinglyLinked: SinglyLinkedType<unknown> = {
    head: {
        data: null,
        next: null
    },

    insertNodeAtEnd(node: Node<unknown>) {
        console.log(this, node)
    }

    print() {

        let curr = this.head

        while (curr.next !== null) {
            console.log(curr.data)
            curr = curr.next
        }
    }
}

export default SinglyLinked
